@startuml

title __SRC's Class Diagram__\n

  package  {
    class AdminUser {
        + AdminUser()
    }
  }
  

  package  {
    interface Editable {
        {abstract} + EditFile()
    }
  }
  

  package  {
    abstract class File {
        - author : String
        - description : String
        - fileSize : float
        - typeOfFile : String
        {static} + threshold : float
        + File()
    }
  }
  

  package  {
    class MediaFile {
        - height : int
        - length : int
        {static} + heightThreshold : int
        {static} + lengthThreshold : int
        + MediaFile()
        + EditFile()
    }
  }
  

  package  {
    class NotRegisteredUser {
        + NotRegisteredUser()
    }
  }
  

  package  {
    class Pdf {
        + Pdf()
    }
  }
  

  package  {
    class Picture {
        + Picture()
    }
  }
  

  package  {
    class Program {
        {static} + main()
    }
  }
  

  package  {
    class RegularUser {
        + RegularUser()
    }
  }
  

  package  {
    class TextDocument {
        + TextDocument()
    }
  }
  

  package  {
    abstract class User {
        {static} - counter : int
        - name : String
        - surname : String
        - address : String
        - author : String
        - id : int
        - accessLevel : int
        + getAccessLevel()
        + setAccessLevel()
        + getName()
        + User()
        + createPdf()
        + createWord()
        + createVideo()
        + createPicture()
    }
  }
  

  package  {
    class Validator {
        + isSizeIsFine()
        + isResolutionFine()
        + isAccessLevelFine()
    }
  }
  

  package  {
    class Video {
        - duration : double
        + Video()
    }
  }
  

  package  {
    class Word {
        + Word()
        + EditFile()
    }
  }
  

  AdminUser -up-|> RegularUser
  File o-- User : user
  MediaFile -up-|> Editable
  MediaFile -up-|> File
  NotRegisteredUser -up-|> User
  Pdf -up-|> TextDocument
  Picture -up-|> MediaFile
  RegularUser -up-|> NotRegisteredUser
  TextDocument -up-|> File
  User o-- Validator : validator
  Video -up-|> MediaFile
  Word -up-|> Editable
  Word -up-|> TextDocument


